<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="AddCoverMethodForm" type="org.vegbank.ui.struts.AddCoverMethodForm" />
		<form-bean name="AddJournalForm" type="org.vegbank.ui.struts.AddJournalForm" />
		<form-bean name="AddPartyForm" type="org.vegbank.ui.struts.AddPartyForm" />
		<form-bean name="AddReferenceForm" type="org.vegbank.ui.struts.AddReferenceForm" />
		<form-bean name="AddStratumMethodForm" type="org.vegbank.ui.struts.AddStratumMethodForm" />
		<form-bean name="CertificationForm" type="org.vegbank.ui.struts.CertificationForm" />
		<form-bean name="ChangePasswordForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="password" type="java.lang.String" />
			<form-property name="newpassword1" type="java.lang.String" />
			<form-property name="newpassword2" type="java.lang.String" />
			<form-property name="action" type="java.lang.String" />
		</form-bean>
		<form-bean name="CommQueryForm" type="org.vegbank.ui.struts.CommQueryForm" />
		<form-bean name="DisplayEntityForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="accessionCode" type="java.lang.String" />
			<form-property name="resultType" type="java.lang.String" />
		</form-bean>
		<form-bean name="DisplayPlotForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="accessionCode" type="java.lang.String" />
			<form-property name="resultType" type="java.lang.String" />
		</form-bean>
		<form-bean name="DisplayQueryResultsForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="selectedPlots" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="DownLoadForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="dataType" type="java.lang.String" initial="all" />
			<form-property name="formatType" type="java.lang.String" initial="xml" />
			<form-property name="selectedPlots" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="DownLoadManagerForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="selectedPlots" type="java.lang.String[]" />
		</form-bean>
		<form-bean name="DropPlotForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="plotIdList" type="java.lang.String" />
		</form-bean>
		<form-bean name="EmailPasswordForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="email" type="java.lang.String" />
		</form-bean>
		<form-bean name="EntityNameForm" type="org.vegbank.ui.struts.EntityNameForm" />
		<form-bean name="InterpretPlantForm" type="org.vegbank.ui.struts.InterpretPlantForm" />
		<form-bean name="LogonForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="username" type="java.lang.String" />
			<form-property name="password" type="java.lang.String" />
		</form-bean>
		<form-bean name="GenericDispatcherForm" type="org.apache.struts.action.DynaActionForm">
			<form-property name="accessionCode" type="java.lang.String" />
		</form-bean>
		<form-bean name="PlantQueryForm" type="org.vegbank.ui.struts.PlantQueryForm" />
		<form-bean name="PlotQueryForm" type="org.vegbank.ui.struts.PlotQueryForm" />
		<form-bean name="RegisterNewUserForm" type="org.apache.struts.validator.DynaValidatorForm">
			<form-property name="usr" type="org.vegbank.common.model.Usr" />
			<form-property name="party" type="org.vegbank.common.model.Party" />
			<form-property name="termsaccept" type="java.lang.String" initial="decline" />
			<form-property name="password1" type="java.lang.String" />
			<form-property name="password2" type="java.lang.String" />
		</form-bean>
		<form-bean name="UploadPlotForm" type="org.vegbank.ui.struts.UploadPlotForm" />
		<form-bean name="UserProfileForm" type="org.vegbank.ui.struts.UserProfileForm" />
	</form-beans>
	<global-forwards>
		<forward name="failure" path="/forms/failure.jsp" redirect="false" />
		<forward name="success" path="/forms/success.html" />
		<forward name="vberror" path="/general/vberror.jsp" />
		<forward name="AddCoverMethod" path="/forms/AddCoverMethod.jsp" />
		<forward name="AddJournal" path="/forms/AddJournal.jsp" />
		<forward name="AddParty" path="/forms/AddParty.jsp" />
		<forward name="AddReference" path="/forms/AddReference.jsp" />
		<forward name="AddStratumMethod" path="/forms/AddStratumMethod.jsp" />
		<forward name="AdminMenu" path="/admin/admin_menu.jsp" />
		<forward name="AuthenticationException" path="/forms/AuthenticationException.jsp" />
		<forward name="Certification" path="/LoadCertification.do" />
		<forward name="CertificationException" path="/forms/CertificationException.jsp" />
		<forward name="CertificationList" path="/ListCertifications.do" />
		<forward name="ChangePassword" path="/forms/change_pwd.jsp" redirect="false" />
		<forward name="InterpretTaxonObservation" path="/InterpretTaxonObservation.do" redirect="false" />
		<forward name="DownLoad" path="/forms/DownLoad.jsp" />
		<forward name="DownLoadManager" path="/DownLoadManager.do" />
		<forward name="DisplayLoadReport" path="/forms/DisplayLoadReport.jsp" redirect="true" />
		<forward name="DisplayResults" path="/forms/DisplayResults.jsp" />
		<forward name="DisplayUploadPlotAction" path="/forms/plot-submit.jsp" />
		<forward name="DropPlotAction" path="/forms/plot-drop.jsp" />
		<forward name="EmailPassword" path="/forms/EmailPassword.jsp" />
		<forward name="EntityName" path="/forms/EntityName.jsp" />
		<forward name="Logoff" path="/forms/Logoff.do" />
		<forward name="Logon" path="/general/login.jsp" />
		<forward name="MainMenu" path="/forms/MainMenu.jsp" />
		<forward name="ManageUsers" path="/admin/manage_users.jsp" redirect="false" />
		<forward name="PlantQuery" path="/forms/PlantQuery.jsp" />
		<forward name="CommQuery" path="/forms/CommQuery.jsp" />
		<forward name="PleaseWait" path="/PleaseWait.do" />
		<forward name="PlotQuery" path="/PlotQuery.do" redirect="false" contextRelative="false" />
		<forward name="LoadPlotQuery" path="/LoadPlotQuery.do" redirect="false" />
		<forward name="RegisterNewUser" path="/forms/RegisterNewUser.jsp" />
	</global-forwards>
	<action-mappings type="org.vegbank.ui.struts.VegbankActionMapping">
		<action path="/AdminMenu" forward="/admin/admin_menu.jsp" />
		<action path="/AddCoverMethod" type="org.vegbank.ui.struts.AddCoverMethodAction" name="AddCoverMethodForm" scope="request" validate="true" input="AddCoverMethod" />
		<action path="/AddJournal" type="org.vegbank.ui.struts.AddJournalAction" name="AddJournalForm" scope="request" validate="true" input="AddJournal" />
		<action path="/AddParty" type="org.vegbank.ui.struts.AddPartyAction" name="AddPartyForm" scope="request" validate="true" input="AddParty" />
		<action path="/AddReference" type="org.vegbank.ui.struts.AddReferenceAction" name="AddReferenceForm" scope="request" validate="true" input="AddReference">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="pro" />
		</action>
		<action path="/AddStratumMethod" type="org.vegbank.ui.struts.AddStratumMethodAction" name="AddStratumMethodForm" scope="request" validate="true" input="AddStratumMethod" />
		<action path="/DisplayMainMenu" forward="/forms/MainMenu.jsp">
		</action>
		<action path="/DisplayEntity" type="org.vegbank.ui.struts.DisplayEntityAction" name="DisplayEntityForm" scope="request" validate="true" input="failure" />
		<action path="/DisplayPlot" type="org.vegbank.ui.struts.DisplayPlotAction" name="DisplayPlotForm" scope="request" validate="true" input="failure" />
		<action path="/DisplayUploadPlotAction" forward="/forms/plot-submit.jsp">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="reg" />
		</action>
		<action path="/CommQuery" type="org.vegbank.ui.struts.QueryAction" name="CommQueryForm" scope="request" validate="true" input="CommQuery">
			<forward name="success" path="/forms/DisplayQueryResults.jsp" />
		</action>
		<action path="/DownLoad" type="org.vegbank.ui.struts.DownLoadAction" name="DownLoadForm" scope="request" validate="true" input="DownLoad" />
		<action path="/DownLoadManager" type="org.vegbank.ui.struts.DownLoadManagerAction" name="DownLoadManagerForm" scope="request" validate="true" input="DisplayResults">
			<forward name="success" path="/forms/DownLoad.jsp" />
		</action>
		<action path="/DropPlot" type="org.vegbank.ui.struts.DropPlotAction" name="DropPlotForm" scope="request" validate="true" input="DropPlotAction">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="dba" />
		</action>
		<action path="/EmailPassword" type="org.vegbank.ui.struts.EmailPasswordAction" name="EmailPasswordForm" scope="request" validate="true" input="EmailPassword">
			<forward name="success" path="/index.jsp" />
		</action>
		<action path="/EntityName" type="org.vegbank.ui.struts.EntityNameAction" name="EntityNameForm" scope="request" validate="true" input="EntityName" />
		<action path="/GenericDispatcher" type="org.vegbank.ui.struts.GenericDispatcherAction" name="GenericDispatcherForm" scope="request" />
		<action path="/GenericDispatcherFwd" type="org.vegbank.ui.struts.GenericDispatcherFwdAction" />
		<action path="/InterpretTaxonObservation" parameter="edit" type="org.vegbank.ui.struts.InterpretPlantAction" scope="request" name="InterpretPlantForm" input="InterpretTaxonObservation" validate="false">
			<forward name="edit" path="/forms/TaxonInterpretationAnnotateData.jsp" redirect="false" />
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="pro" />
		</action>
		<action path="/ListCertifications" type="org.vegbank.ui.struts.admin.CertificationListAction" name="CertificationForm" scope="request" validate="false">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="dba" />
			<forward name="view" path="/admin/list_certifications.jsp" redirect="false" />
		</action>
		<action path="/LoadCertification" type="org.vegbank.ui.struts.CertificationLoadAction" name="CertificationForm" scope="request" validate="false">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="reg" />
			<forward name="success" path="/forms/certification.jsp" />
		</action><action path="/LoadPlotQuery" type="org.vegbank.ui.struts.PlotQueryLoadAction" name="PlotQueryForm" validate="false" scope="request">
			<forward name="plot_query" path="/forms/plot-query.jsp" redirect="false" />
		</action><action path="/LoadUser" parameter="user" type="org.vegbank.ui.struts.UserProfileAction" name="UserProfileForm" validate="false" scope="session">
			<set-property property="action" value="edit" />
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="reg" />
			<forward name="view_user" path="/forms/edit_user.jsp" redirect="false" />
			<forward name="edit_user" path="/forms/edit_user.jsp" redirect="false" />
		</action><action path="/Logoff" type="org.vegbank.ui.struts.LogoffAction">
			<forward name="success" path="/index.jsp" />
		</action>
		<action path="/Logon" type="org.vegbank.ui.struts.LogonAction" name="LogonForm" validate="true" input="Logon">
			<forward name="success" path="/forms/MainMenu.jsp" redirect="true" />
		</action>
		
		
		
		<action path="/PlantQuery" type="org.vegbank.ui.struts.QueryAction" name="PlantQueryForm" scope="request" validate="true" input="PlantQuery">
			<forward name="success" path="/forms/DisplayQueryResults.jsp" />
		</action>
		<action path="/PleaseWait" type="org.vegbank.ui.struts.PleaseWaitAction" scope="request" validate="false">
			<forward name="self" path="/general/wait.jsp" />
		</action>
		<action path="/PlotQuery" type="org.vegbank.ui.struts.PlotQueryAction" name="PlotQueryForm" scope="request" validate="true" input="LoadPlotQuery">
			<forward name="display_results" path="/forms/DisplayResults.jsp" redirect="false" />
		</action>
		<action path="/RegisterNewUser" type="org.vegbank.ui.struts.RegisterNewUserAction" name="RegisterNewUserForm" scope="request" validate="true" input="RegisterNewUser">
			<forward name="success" path="/DisplayMainMenu.do" redirect="true" />
		</action>
		<action path="/SaveCertification" type="org.vegbank.ui.struts.CertificationSaveAction" name="CertificationForm" scope="request" input="Certification" validate="true">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="reg" />
			<forward name="failure" path="/LoadCertification.do" redirect="false" />
		</action>
		<action path="/SaveTaxonInterpretation" validate="true" parameter="save" type="org.vegbank.ui.struts.InterpretPlantAction" name="InterpretPlantForm"  input="InterpretTaxonObservation" scope="request">
			<forward name="edit" path="/forms/TaxonInterpretationAnnotateData.jsp" redirect="false" />
		</action>
		<action path="/UpdatePassword" type="org.vegbank.ui.struts.UserProfileAction" name="ChangePasswordForm" parameter="pwd" scope="request" validate="true" input="ChangePassword">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="reg" />
			<forward name="change_pwd" path="/forms/change_pwd.jsp" redirect="false" />
		</action>
		<action path="/UploadPlot" type="org.vegbank.ui.struts.UploadPlotAction" name="UploadPlotForm" scope="session" validate="true" input="DisplayUploadPlotAction">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="cert" />
		</action>
		<action path="/UpdateUser" name="UserProfileForm" scope="session" validate="true" parameter="user" input="edit_user" type="org.vegbank.ui.struts.UserProfileAction">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="reg" />
			<set-property property="action" value="update" />
			<forward name="edit_user" path="/forms/edit_user.jsp" />
		</action>
		<action path="/ViewCertification" type="org.vegbank.ui.struts.admin.CertificationViewAction" name="CertificationForm" scope="request" validate="false">
			<set-property property="authClass" value="org.vegbank.ui.struts.Authenticate" />
			<set-property property="reqRoles" value="dba" />
			<forward name="view" path="/admin/view_certification.jsp" />
			<forward name="list" path="/ListCertifications.do" redirect="true" />
		</action>
	</action-mappings>
	<controller processorClass="org.vegbank.ui.struts.RequestProcessor">
		<set-property property="inputForward" value="true" />
		<set-property property="maxFileSize" value="20M" />
	</controller>
	<message-resources parameter="org.apache.struts.webapp.example.ApplicationResources" />
	<plug-in className="org.apache.struts.plugins.ModuleConfigVerifier" />
	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames" value="/WEB-INF/validator-rules.xml, /WEB-INF/validation.xml" />
	</plug-in>
	<plug-in className="org.vegbank.ui.struts.VegbankInitPlugin" />
</struts-config>
