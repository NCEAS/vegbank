<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	


<title> Prototype Veg Plots Database Component Description: communityUsage </title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description>This table stores information about the vegetation classification applied to a specific plotObservation 
by a specific party.</description>
<date></date>

<entity>
	<entityName>vegPlot.communityUsage </entityName>
	<entitySummary>This table tracks the application of vegetation community names to plot observations by 
	differing parties over time.</entitySummary>
	<entityDescription> </entityDescription>

        <attribute>
                <attName>COMMUNITYUSAGE_ID</attName>
                <attNulls>No </attNulls>
                <attType>NUMBER </attType>
                <attKey>PK </attKey>
		<attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Primary Key for the communityUsage table.</attDefinition>
        </attribute>


	<attribute> 
		<attName>PARTY_ID</attName>
		<attNulls>No </attNulls>	
		<attType>NUMBER </attType>
		<attKey>FK </attKey>
		<attReferences>PARTY </attReferences>
		<attNotes>Foreign key into the party table.</attNotes>
		<attDefinition>This key links the application of a veg class to a plot by a given party identified by this key. </attDefinition>
	</attribute>



	<attribute>
                <attName>OBS_ID</attName>
                <attNulls>No </attNulls>
                <attType>NUMBER </attType>
                <attKey>FK</attKey>
                <attReferences>plotObservation</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Foreign Key into the plotObservation table. </attDefinition>
        </attribute>

	<attribute>
                <attName>COMMUNITYTYPE_ID</attName>
                <attNulls>No </attNulls>
                <attType>NUMBER </attType>
                <attKey>FK</attKey>
                <attReferences>communityType</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Foreign Key into the communityType table. </attDefinition>
        </attribute>
	
	<attribute>
                <attName>type</attName>
                <attNulls>No </attNulls>
                <attType>BOOLEAN </attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Specifies if the plot observation was used in defining the classification type.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>startDate</attName>
                <attNulls>No </attNulls>
                <attType>DATE </attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Start date for the application of a vegetation class to a plot observation by a given party. </attDefinition>
        </attribute>
	
	<attribute>
                <attName>stopDate</attName>
                <attNulls>No </attNulls>
                <attType>DATE </attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Stop date for the application of a vegetation class to a plot observation by a given party.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>classQuality</attName>
                <attNulls>Yes</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>This attribute is to store the classification quality associated with 
		the community usage.  Although it is pretty subjective field it is meaningful in many
		 cases and would be bad to lose the record. </attDefinition>
        </attribute>
	
	

	<children> </children>
</entity>
</dataModel>	
