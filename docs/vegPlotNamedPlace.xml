<entity>
	<entityName> vegPlot.NAMEDPLACE</entityName>
	<entitySummary>This tables stores information about the various 'named places' that a plot may have been 
	sample within.</entitySummary>
	<entityDescription> </entityDescription>
	<attribute> 
		<attName>namedPlaceId</attName>
		<attNulls>No</attNulls>	
		<attType>NUMBER</attType>
		<attKey>P</attKey>
		<attNotes>n/a </attNotes>
		<attDefinition>Internal database unique value assigned to every named place 
		entered in the database.</attDefinition>
	</attribute>



	<attribute>
                <attName>placeName </attName>
                <attNulls>No</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>Name of a place like: Yosemitie National Park. </attNotes>
                <attDefinition>The name of the place at which a plot was sampled. </attDefinition>
        </attribute>

	
	<attribute>
                <attName>placeDescription </attName>
                <attNulls>No</attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
                <attReferences> </attReferences>
                <attNotes></attNotes>
                <attDefinition>This is the description of a named place. </attDefinition>
        </attribute>

	
	<attribute>
                <attName>gazetterRef </attName>
                <attNulls>Yes</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a </attKey>
                <attReferences> </attReferences>
                <attNotes></attNotes>
                <attDefinition>This is a reference to a gazeteer containing the named place.</attDefinition>
        </attribute>
	
	<children> </children>
</entity>
