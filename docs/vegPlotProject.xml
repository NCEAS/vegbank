<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	
<title>>Prototype Veg Plots Database Component Description: project </title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description> </description>
<date></date>

<entity>
	<entityName>vegPlot.PROJECT</entityName>
	<entitySummary> This table stores information about a project established to collect vegetation plot data.</entitySummary>
	<entityDescription>The Project table is the table wich contains all the 'high' level general
	project information.</entityDescription>
	<attribute> 
		<attName>PROJECT_ID </attName>
		<attNulls>No </attNulls>	
		<attType>NUMBER </attType>
		<attKey>PK</attKey>
		<attReferences>n/a</attReferences>
		<attNotes>n/a</attNotes>
		<attDefinition>Database assigned value for a given project.</attDefinition>
	</attribute>

	<attribute>
                <attName>projectName </attName>
                <attNulls>No </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>Project name as defined by the PI.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>description</attName>
                <attNulls>Yes</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition> Short description of the project including the purpose for doing the project.</attDefinition>
        </attribute>

	
	<!-- added on Oct 19 -->
	
	<attribute>
                <attName>startDate</attName>
                <attNulls>Yes</attNulls>
                <attType>DATE</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Project start date.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>stopDate</attName>
                <attNulls>Yes</attNulls>
                <attType>DATE</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Project stop date.</attDefinition>
        </attribute>
	
	
	
	
	<!-- nn -->



	<children> </children>
</entity>
</dataModel>	
