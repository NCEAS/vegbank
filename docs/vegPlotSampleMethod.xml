<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	


<title> Prototype Veg Plots Database Component Description: sampleMethod </title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description>This table associates the sampling method used for plot data collection with a reference 
describing that sampling method.</description>
<date></date>

<entity>
	<entityName>vegPlot.sampleMethod </entityName>
	<entitySummary>This table stores the email addresses of the various parties involved in the collection of plots. </entitySummary>
	<entityDescription> </entityDescription>

        <attribute>
                <attName>sampleMethodId </attName>
                <attNulls>No </attNulls>
                <attType>NUMBER </attType>
                <attKey>PK </attKey>
		<attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Primary key for the sampleMethod table.</attDefinition>
        </attribute>


	<attribute> 
		<attName>sampleMethodName </attName>
		<attNulls>No </attNulls>	
		<attType>VARCHAR2 </attType>
		<attKey>FK </attKey>
		<attReferences>PARTY </attReferences>
		<attNotes>n/a</attNotes>
		<attDefinition>This is the name of the sampling method.</attDefinition>
	</attribute>



	<attribute>
                <attName>sampleMethodDescription </attName>
                <attNulls>No </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Brief description of the sampling method.</attDefinition>
        </attribute>

	<attribute>
                <attName>sampleMethodCitation </attName>
                <attNulls>No </attNulls>
                <attType>NUMBER </attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Foreign Key into the citation table identifying the reference used to describe the sampling method.</attDefinition>
        </attribute>

	
	
	<children> </children>
</entity>
</dataModel>	
