<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	
<title>Prototype Veg Plots Database Component Description: communityUsage</title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description> </description>
<date></date>



<entity>
	<entityName>community.communityUsage</entityName>
	<entitySummary>This table links a community name with a concept.</entitySummary>
	<entityDescription> </entityDescription>
	<attribute> 
		<attName>COMMUNITYUSAGE_ID</attName>
		<attNulls>No</attNulls>	
		<attType>NUMBER</attType>
		<attKey>PK</attKey>
		<attReferences>n/a</attReferences>
		<attNotes>n/a</attNotes>
		<attDefinition>Database assigned value to each unique record in 
		the communityUsage table.</attDefinition>
	</attribute>



	<attribute>
                <attName>COMMUNITYNAME_ID</attName>
                <attNulls>No</attNulls>
                <attType>NUMBER</attType>
                <attKey>FK</attKey>
                <attReferences>communityName</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This field is the foreign key into the communityName 
		table.</attDefinition>
        </attribute>

	<attribute>
                <attName>COMMUNITYCONCEPT_ID</attName>
                <attNulls>No</attNulls>
                <attType>NUMBER</attType>
                <attKey>FK</attKey>
		<attReferences>communityConcept</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This field is the foreign key into the communityConcept 
		table.</attDefinition>
        </attribute>

	<attribute>
                <attName>usageStart</attName>
                <attNulls>NO</attNulls>
                <attType>DATE</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the date on which a name was first applied 
		to a concept.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>usageStop</attName>
                <attNulls>NO</attNulls>
                <attType>DATE</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the date on which a name no longer was being 
		applied to a concept.</attDefinition>
        </attribute>
	
	<attribute> 
		<attName>communityNameStatus</attName>
		<attNulls>No</attNulls>	
		<attType>VARCHAR2</attType>
		<attKey>n/a</attKey>
		<attReferences>n/a</attReferences>
		<attNotes>n/a</attNotes>
		<attDefinition>This field stores an indicator of whether a community 
		name is of standard status or not per the party designated in the 
		next field.</attDefinition>
	</attribute>
	
	<attribute>
                <attName>party</attName>
                <attNulls>Yes</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a</attKey>
		<attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the name of the party that uses the class 
		concept with the name.</attDefinition>
        </attribute>

	<attribute>
                <attName>classSystem</attName>
                <attNulls>Yes</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a</attKey>
		<attReferences>n/a</attReferences>
                <attNotes>Like FGDC of Forest Cover Class.</attNotes>
                <attDefinition>This is the name of the system that the class (from 
		the communityConcept) falls into.</attDefinition>
        </attribute>

	
	


	<children> </children>
</entity>
</dataModel>	
