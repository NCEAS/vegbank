<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	


<title> Prototype Veg Plots Database Component Description: email </title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description> </description>
<date></date>

<entity>
	<entityName>vegPlot.EMAIL </entityName>
	<entitySummary>This table stores the email addresses of the various parties involved in the collection of plots. </entitySummary>
	<entityDescription> </entityDescription>

        <attribute>
                <attName>email_id </attName>
                <attNulls>No </attNulls>
                <attType>NUMBER </attType>
                <attKey>P </attKey>
                <attNotes>n/a </attNotes>
                <attDefinition>Database assigned value for a unique email address. </attDefinition>
        </attribute>


	<attribute> 
		<attName>party_id </attName>
		<attNulls>No </attNulls>	
		<attType>NUMBER </attType>
		<attKey>F </attKey>
		<attReferences>PARTY </attReferences>
		<attNotes>Foreign key into the party table.</attNotes>
		<attDefinition>This key links an email address(s) to a given party. </attDefinition>
	</attribute>



	<attribute>
                <attName>emailAddress </attName>
                <attNulls>No </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes> </attNotes>
                <attDefinition>The email address. </attDefinition>
        </attribute>


	<children> </children>
</entity>
</dataModel>	
