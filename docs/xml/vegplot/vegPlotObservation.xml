		</attribute>
		<attribute>
			<attName>stemObservationArea</attName>
			<attNulls>Yes</attNulls>
			<attType>NUMERIC(5,0)</attType>
			<attKey>n/a</attKey>
			<attReferences>n/a</attReferences>
