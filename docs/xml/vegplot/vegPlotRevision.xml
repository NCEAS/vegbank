<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">
<dataModel>
	<title>Vegetation Plots Database Component Description: revision </title>
	<author>J. Harris </author>
	<institution>NCEAS </institution>
	<contributor>R.K. Peet</contributor>
	<description>This table stores information about changes made to the database records.</description>
	<date>31 October 2001</date>
	<entity>
		<entityName>vegPlot.revision </entityName>
		<entitySummary>This table constitutes a log of  changes in data deposited in the database. </entitySummary>
		<entityDescription>This table was designed to store as a single, stand-alone table information about attributes in any of the database tables.</entityDescription>
		<attribute>
			<attName>REVISION_ID</attName>
			<attNulls>No</attNulls>
			<attType>INT</attType>
			<attKey>PK </attKey>
			<attReferences>n/a</attReferences>
			<attNotes>Primary key for the revision table</attNotes>
			<attDefinition>Database generated identifier assigned to each unique revision. </attDefinition>
		</attribute>
		<attribute>
			<attName>tableName</attName>
			<attNulls>No</attNulls>
			<attType>VARCHAR(50)</attType>
			<attKey>n/a</attKey>
			<attReferences>n/a</attReferences>
			<attNotes>n/a</attNotes>
			<attDefinition>Name of the table where the change occurred.</attDefinition>
		</attribute>
		<attribute>
			<attName>tableAttribute </attName>
			<attNulls>No</attNulls>
			<attType>VARCHAR(50)</attType>
			<attKey>n/a </attKey>
			<attReferences>n/a </attReferences>
			<attNotes>n/a </attNotes>
			<attDefinition>The name of the attribute where the chance occurred. </attDefinition>
		</attribute>
		<attribute>
			<attName>tableRecord</attName>
			<attNulls>No</attNulls>
			<attType>INT</attType>
			<attKey>n/a </attKey>
			<attReferences>n/a </attReferences>
			<attNotes>n/a </attNotes>
			<attDefinition>The number of the record in which the change occurred (i.e., the value of the primary key associated with the value in which the change occured)</attDefinition>
		</attribute>
		<attribute>
			<attName>revisionDate</attName>
			<attNulls>No</attNulls>
			<attType>DATE</attType>
			<attKey>n/a</attKey>
			<attReferences>n/a </attReferences>
			<attNotes>n/a </attNotes>
			<attDefinition>The date on which the revision was made.</attDefinition>
		</attribute>
		<attribute>
			<attName>previousValueText</attName>
			<attNulls>No</attNulls>
			<attType>VARCHAR(500)</attType>
			<attKey>n/a </attKey>
			<attReferences>n/a </attReferences>
			<attNotes>n/a </attNotes>
			<attDefinition>The previous value as a character string.</attDefinition>
		</attribute>
		<attribute>
			<attName>previousValueType</attName>
			<attNulls>No</attNulls>
			<attType>VARCHAR(20)</attType>
			<attKey>n/a</attKey>
			<attReferences>n/a </attReferences>
			<attNotes>n/a </attNotes>
			<attDefinition>The data type of the attribute changed.</attDefinition>
		</attribute>
		<attribute>
			<attName>previousRevision_ID</attName>
			<attNulls>Yes</attNulls>
			<attType>INT</attType>
			<attKey>FK</attKey>
			<attReferences>revision.REVISION_ID</attReferences>
			<attNotes>Recursive foreign key</attNotes>
			<attDefinition>Pointer to a previous record of a revision.</attDefinition>
		</attribute>
		<children/>
	</entity>
</dataModel>
