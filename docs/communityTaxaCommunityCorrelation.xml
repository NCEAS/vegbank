<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	
<title>Prototype Veg Plots Database Component Description: communityCorrelation</title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description> </description>
<date></date>



<entity>
	<entityName>community.communityCorrelation</entityName>
	<entitySummary>This table correlates multiple community concepts as they 
	change through time.</entitySummary>
	<entityDescription> </entityDescription>
	<attribute> 
		<attName>COMMUNITYCORRELATION_ID</attName>
		<attNulls>NO</attNulls>	
		<attType>NUMBER</attType>
		<attKey>PK</attKey>
		<attReferences>n/a</attReferences>
		<attNotes>n/a</attNotes>
		<attDefinition>Database assigned value to each unique record in 
		the communityCorrelation table.</attDefinition>
	</attribute>



	<attribute>
                <attName>CONCEPT1_ID</attName>
                <attNulls>NO</attNulls>
                <attType>NUMBER</attType>
                <attKey>n/a</attKey>
                <attReferences>communityConcept</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the foreign key into the communityConcept table 
		"pointing" to the earlier concept which correlates with a 
		concept that used more recently.</attDefinition>
        </attribute>

	<attribute>
                <attName>CONCEPT2_ID</attName>
                <attNulls>NO</attNulls>
                <attType>NUMBER</attType>
                <attKey>n/a</attKey>
                <attReferences>communityConcept</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the foreign key into the communityConcept table 
		"pointing" to the more recent concept that correlates with an older 
		concept usage.</attDefinition>
        </attribute>


	<attribute>
                <attName>convergence</attName>
                <attNulls>NO</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the descriptive attribute that is used to 
		relate the congruence between two concepts.</attDefinition>
        </attribute>

	<attribute>
                <attName>correlationStart</attName>
                <attNulls>NO</attNulls>
                <attType>DATE</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the start date for an occurence of a correlation 
		between two concepts.</attDefinition>
        </attribute>
	
	
	<attribute>
                <attName>correlationStop</attName>
                <attNulls>NO</attNulls>
                <attType>DATE</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a</attReferences>
                <attNotes>n/a</attNotes>
                <attDefinition>This is the stop date for an occurence of a correlation 
		between two concepts.</attDefinition>
        </attribute>

	<children> </children>
</entity>
</dataModel>	
