<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	


<title> Prototype Veg Plots Database Component Description: revisions </title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description>This table stores information about cahanges meade to the database records.</description>
<date></date>

<entity>
	<entityName>vegPlot.REVISIONS </entityName>
	<entitySummary>This table stores information about changes made to the plots database. </entitySummary>
	<entityDescription>As with the tables realted to the user defined attributes and the tables associated 
	with the plot notations, this table was designed to store information about attributes in any of the 
	database tables in a stand alone table.</entityDescription>

        <attribute>
                <attName>REVISION_ID</attName>
                <attNulls>No</attNulls>
                <attType>NUMBER </attType>
                <attKey>PK </attKey>
		<attReferences>n/a</attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Foreign key in the revision table.</attDefinition>
        </attribute>


	<attribute> 
		<attName>tableName</attName>
		<attNulls>No</attNulls>	
		<attType>VARCHAR2</attType>
		<attKey>n/a</attKey>
		<attReferences>n/a</attReferences>
		<attNotes>n/a</attNotes>
		<attDefinition>Name of the table where the changes have occurred.</attDefinition>
	</attribute>



	<attribute>
                <attName>tableAttribute </attName>
                <attNulls>No</attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>The name of the attribute where the changes occurred. </attDefinition>
        </attribute>

	<attribute>
                <attName>tableRecord</attName>
                <attNulls>No</attNulls>
                <attType>NUMBER</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>The row level that the change occurred.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>revisionDate</attName>
                <attNulls>No</attNulls>
                <attType>DATE</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>The date on which the revision was made.</attDefinition>
        </attribute>

	
	<attribute>
                <attName>previousValueText</attName>
                <attNulls>No</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>The previous value as a character string.</attDefinition>
        </attribute>
	

	
	<attribute>
                <attName>previousValueType</attName>
                <attNulls>No</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a</attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>The data type of the attribute changed.</attDefinition>
        </attribute>
	
	<attribute>
                <attName>revisedValueText</attName>
                <attNulls>No</attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>The value (as a character string) of the new attribute.</attDefinition>
        </attribute>
	<children> </children>
	
	<attribute>
                <attName>previousREVISION_ID</attName>
                <attNulls>Yes</attNulls>
                <attType>NUMBER</attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Foreign key pointing to a previous revisionId record.</attDefinition>
        </attribute>
</entity>
</dataModel>	
