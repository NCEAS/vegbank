<?xml version="1.0"?>
<!DOCTYPE dataModel SYSTEM "tableStructure.dtd">

<dataModel>	

<title> Prototype Veg Plots Database Component Description: onlineResource</title>
<author>J. Harris </author>
<institution>NCEAS </institution>
<contributor> </contributor>
<description> </description>
<date></date>



<entity>
	<entityName>vegPlot.ONLINERESOURCE </entityName>
	<entitySummary>This table stores information about a party's online resources. </entitySummary>
	<entityDescription> </entityDescription>
	<attribute> 
		<attName>ONLINERESOURCE_ID</attName>
		<attNulls>No </attNulls>	
		<attType>NUMBER </attType>
		<attKey>PK </attKey>
		<attReferences>n/a </attReferences>
		<attNotes>Primary key for onlineResource table. </attNotes>
		<attDefinition>Database assigned value to each unique record in the onlineResource table. </attDefinition>
	</attribute>



	<attribute>
                <attName>PARTY_ID</attName>
                <attNulls>No</attNulls>
                <attType>NUMBER</attType>
                <attKey>FK </attKey>
                <attReferences>vegPlotParty </attReferences>
                <attNotes>Foreign key into the PARTY table. </attNotes>
                <attDefinition>Database assigned value that is unique to each party
                                  contributing in the collection of plots.
		 </attDefinition>
        </attribute>


	<attribute>
                <attName>linkage</attName>
                <attNulls>Yes </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
		<attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Location (address) for on-line access using a
                                  Uniform Resource Locator address or similar
                                  addressing scheme such as:
                                  http://www.nceas.ucsb.edu.
		 </attDefinition>
        </attribute>



        <attribute>
                <attName>protocol </attName>
                <attNulls>Yes </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Connection protocol to be used. </attDefinition>
        </attribute>
	
	<attribute>
                <attName>name </attName>
                <attNulls>No </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
		<attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Name of the resource.</attDefinition>
        </attribute>



        <attribute>
                <attName>applicationProfile </attName>
                <attNulls>Yes </attNulls>
                <attType>VARCHAR2 </attType>
                <attKey>n/a </attKey>
                <attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Name of the application profile that can be used with the resource. </attDefinition>
        </attribute>

	<attribute>
                <attName>description </attName>
                <attNulls>Yes </attNulls>
                <attType>VARCHAR2</attType>
                <attKey>n/a </attKey>
		<attReferences>n/a </attReferences>
                <attNotes>n/a </attNotes>
                <attDefinition>Description of what the resource is/does. </attDefinition>
        </attribute>





	<children> </children>
</entity>
</dataModel>	
