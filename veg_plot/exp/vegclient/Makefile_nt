GCC = gcc
GCC_FLAGS = -mno-cygwin
all:
	$(GCC) $(GCC_FLAGS) -o ./bin/vegclient.exe  ./src/vegclient_nt.c
	$(GCC)  -o ./bin/PostgresManager.exe   ./src/PostgresManager.c
	$(GCC)  -o ./bin/configure.exe   ./src/configure.c

jar:
        

clean:
        

